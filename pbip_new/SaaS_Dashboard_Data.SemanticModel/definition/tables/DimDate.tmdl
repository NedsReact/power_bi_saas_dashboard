	partition DimDate = calculated
		mode: import
		source =
				
				VAR Dates =
				    FILTER (
				        UNION (
				            SELECTCOLUMNS ( Revenue, "d", Revenue[RevenueDate] ),
				            SELECTCOLUMNS ( Usage, "d", Usage[UsageDate] ),
				            SELECTCOLUMNS ( Subscriptions, "d", Subscriptions[SubscriptionStartDate] ),
				            SELECTCOLUMNS ( Subscriptions, "d", Subscriptions[SubscriptionEndDate] )
				        ),
				        NOT ISBLANK ( [d] )
				    )
				VAR MinDate = MINX ( Dates, [d] )
				VAR MaxDate = MAXX ( Dates, [d] )
				RETURN
				    CALENDAR ( MinDate, MaxDate )

